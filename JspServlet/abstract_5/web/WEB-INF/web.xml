<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>servlet.DbServlet</servlet-name>
        <servlet-class>servlet.DbServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servlet.DbServlet</servlet-name>
        <url-pattern>/qwe</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>servlet.RegServlet</servlet-name>
        <servlet-class>servlet.RegServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servlet.RegServlet</servlet-name>
        <url-pattern>/xyz</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>servlet.ToolServlet</servlet-name>
        <servlet-class>servlet.ToolServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servlet.ToolServlet</servlet-name>
        <url-pattern>/sdf</url-pattern>
    </servlet-mapping>
</web-app>